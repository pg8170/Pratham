<%= render 'shared/errors',obj: @expenses%>
<%= form_for :otherexpenses, :html => {class: "form-horizontal", role: "form"} ,url: { action: :create } do |f|%>
    <div class = 'col-xs-12' >
    <div class ="form-group">
    <div class = "control-lable col-sm-2">
      <%=f.label :YourName%>
    </div>
    <div class="col-sm-8">
      <%=f.text_field :name, class: "form-control", placeholder: "Enter Your Name",autofocus: true%>    
    </div>
    </div>
    <div class ="form-group">
    <div class = "control-lable col-sm-2">
      <%=f.label :Description%>
    </div>
    <div class="col-sm-8">
      <%=f.text_field :description, class: "form-control", placeholder: "Enter the Other Expense description",autofocus: true%>   
    </div>
    </div>
    <div class ="form-group">
    <div class = "control-lable col-sm-2">
      <%=f.label :Month%>
    </div>
    <div class="col-sm-8">
      <%=f.text_field :month, class: "form-control", placeholder: "Enter the month of Bill(e.g January 2016,February 2016 etc)",autofocus: true%>   
    </div>
    </div>
    <div class ="form-group">
    <div class = "control-lable col-sm-2">
      <%=f.label :Amount%>
    </div>
    <div class="col-sm-8">
      <%=f.text_field :amount, class: "form-control", placeholder: "Total Bill in rupees",autofocus: true%>   
    </div>
    </div>
    <div class ="form-group">
    <div class = "col-sm-offset-2 col-sm-10 control-lable col-sm-2">
      <%=f.submit "Save", class: 'btn btn-primary btn-lg'%>
    </div>
    </div>
<%end%>
</div>